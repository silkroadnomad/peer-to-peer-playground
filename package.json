		"@libp2p/webrtc": "^3.2.11",
		"@multiformats/multiaddr": "^12.1.10",
		"@multiformats/multiaddr-matcher": "^1.1.0",
		"blockstore-level": "^1.1.4",
		"datastore-core": "^9.2.3",
		"datastore-level": "^10.1.4",
		"helia": "^2.1.0",
		"it-length-prefixed": "^9.0.3",
		"libp2p": "^0.46.21",
		"multiformats": "^12.1.3",
		"qrcode-generator": "^1.4.4"
	}
